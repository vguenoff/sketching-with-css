{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
  },
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "space-before-function-paren": [0, {
      "anonymous": "always"
    }],
    "func-names": 0,
    "no-unused-vars": 1,
    "no-multiple-empty-lines": 1,
    "class-methods-use-this": 1,
    "comma-dangle": 0,
    "no-restricted-syntax": 0,
    "no-confusing-arrow": 1,
    "prefer-const": 1,
    "no-return-assign": 0,
  }
}
